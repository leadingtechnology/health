# Example Environment Configuration
# Copy this file to .env and update with your actual values

# Database Configuration
ConnectionStrings__DefaultConnection=Host=YOUR_DB_HOST;Port=5432;Database=YOUR_DB_NAME;Username=YOUR_DB_USER;Password=YOUR_DB_PASSWORD;Search Path=health;

# Security Keys (REQUIRED - Generate secure keys!)
# Generate with: openssl rand -base64 32
KEYRING_MASTER_KEY=YOUR_BASE64_ENCODED_32_BYTE_KEY

# Generate with: openssl rand -hex 32
Jwt__SigningKey=YOUR_SECURE_JWT_SIGNING_KEY_AT_LEAST_32_CHARS
Jwt__Issuer=https://api.yourdomain.com
Jwt__Audience=https://app.yourdomain.com
Jwt__ExpireMinutes=1440

# CORS Settings
Cors__AllowedOrigins__0=https://app.yourdomain.com
Cors__AllowedOrigins__1=https://yourdomain.com

# OpenAI Configuration (Optional)
OpenAI__ApiKey=sk-YOUR_OPENAI_API_KEY
OpenAI__DefaultModel=gpt-4o-mini
OpenAI__BaseUrl=https://api.openai.com/v1

# Application Settings
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:5000;https://+:5001
Domain=yourdomain.com

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft=Warning